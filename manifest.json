{
    "name": "Amplitude Controller",
    "description": "Minimized Volume Flux",
    "version": "1.1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "./src/background/background.js"
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "tabCapture"
    ],
    "action": {
      "default_popup": "./src/popup/popup.html",
      "default_icon": {
        "128": "./src/assets/icons/amplitude_controller_icon.png"
      }
    },
    "icons": {
      "128": "./src/assets/icons/amplitude_controller_icon.png"
    }
  }
